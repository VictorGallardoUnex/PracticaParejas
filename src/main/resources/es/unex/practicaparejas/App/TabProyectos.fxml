<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="es.unex.practicaparejas.App.Controladores.TabProyectos"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <HBox layoutX="4.0" layoutY="5.0" prefHeight="347.0" prefWidth="628.0">
            <children>
                <VBox prefHeight="277.0" prefWidth="163.0">
                    <children>
                        <Label prefHeight="27.0" prefWidth="84.0" text="Proyectos" />
                        <TextField promptText="Denominación C" fx:id="denominacionCInput" />
                        <TextField promptText="Denominación L" fx:id="denominacionLInput" />
                        <TextField promptText="Fecha Inicio" fx:id="fechaInicioInput" />
                        <ComboBox fx:id="idServicioComboBox" promptText="ID Servicio" />
                        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="168.0" text="Añadir datos" onAction="#añadirDatos">
                            <VBox.margin>
                                <Insets top="20.0" />
                            </VBox.margin>
                        </Button>
                    </children>
                    <padding>
                        <Insets top="5.0" />
                    </padding>
                </VBox>
                <TableView fx:id="proyectosTableView" prefHeight="347.0" prefWidth="465.0">
                    <columns>
                        <TableColumn prefWidth="100.0" text="Denominación C" fx:id="denominacionCColumn" />
                        <TableColumn prefWidth="100.0" text="Denominación L" fx:id="denominacionLColumn" />
                        <TableColumn prefWidth="100.0" text="Fecha Inicio" fx:id="fechaInicioColumn" />
                        <TableColumn prefWidth="100.0" text="ID Servicio" fx:id="idServicioColumn" />
                    </columns>
                </TableView>
            </children>
        </HBox>
    </children>
</AnchorPane>
